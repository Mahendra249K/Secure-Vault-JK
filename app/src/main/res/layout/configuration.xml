<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/sky_light"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    tools:context=".dashex.Configuration">
    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:layout_alignParentTop="true">
        <include
            android:id="@+id/main_toolbar"
            layout="@layout/it_tool_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </com.google.android.material.appbar.AppBarLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/appbar"
        android:layout_marginStart="@dimen/dim_16"
        android:orientation="vertical"
        android:layout_marginEnd="@dimen/dim_16">
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:scrollbars="none">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dim_3"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:layout_marginTop="@dimen/_12sdp"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="@dimen/_22sdp"
                            android:layout_height="@dimen/_22sdp"
                            android:layout_marginRight="10dp"
                            android:padding="@dimen/_1sdp"
                            android:layout_gravity="center_vertical"
                            android:gravity="center_vertical"
                            android:src="@drawable/changes_image"
                            app:tint="@color/wooden_dark_text" />
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:gravity="center_vertical"
                            android:orientation="vertical">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/app_icon_name_change"
                                android:textColor="@color/wooden_dark_text"
                                android:textSize="@dimen/sip_15" />
                        </LinearLayout>
                    </LinearLayout>



                    <Spinner
                        android:id="@+id/spinner"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:backgroundTint="@color/wooden_dark_text"
                        android:layout_marginTop="@dimen/_3sdp" />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/Alert_Message_for_App"
                        android:layout_gravity="center"
                        android:textColor="@color/red"
                        android:textSize="@dimen/_11sdp"></TextView>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"

                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginBottom="@dimen/_5sdp"
                        android:layout_marginLeft="@dimen/_30sdp"
                        android:background="@color/gray_solid" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/pinchange"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5sdp"
                    android:gravity="center"
                    android:layout_marginTop="@dimen/_7sdp"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="@dimen/_22sdp"
                        android:layout_height="@dimen/_22sdp"
                        android:layout_marginRight="10dp"
                        android:layout_gravity="center_vertical"
                        android:gravity="center_vertical"
                        android:padding="@dimen/_1sdp"
                        android:src="@drawable/password"
                        app:tint="@color/wooden_dark_text" />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:gravity="center_vertical"
                        android:orientation="vertical">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/Change_Password"
                            android:textColor="@color/wooden_dark_text"
                            android:textSize="@dimen/sip_15" />
                    </LinearLayout>
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:layout_marginVertical="@dimen/_5sdp"
                    android:layout_marginLeft="@dimen/_30sdp"
                    android:background="@color/gray_solid" />

                <LinearLayout
                    android:id="@+id/Launguagechange"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5sdp"
                    android:gravity="center"
                    android:layout_marginTop="@dimen/_7sdp"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="@dimen/_22sdp"
                        android:layout_height="@dimen/_22sdp"
                        android:layout_marginRight="10dp"
                        android:padding="@dimen/_1sdp"
                        android:layout_gravity="center_vertical"
                        android:gravity="center_vertical"
                        android:src="@drawable/translation_launguage"
                        app:tint="@color/wooden_dark_text" />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:gravity="center_vertical"
                        android:orientation="vertical">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/Change_Launguage"
                            android:textColor="@color/wooden_dark_text"
                            android:textSize="@dimen/sip_15" />
                    </LinearLayout>
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:layout_marginVertical="@dimen/_5sdp"
                    android:layout_marginLeft="@dimen/_30sdp"
                    android:background="@color/gray_solid" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5sdp"
                    android:layout_marginTop="@dimen/_7sdp"
                    android:gravity="center"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="@dimen/_22sdp"
                        android:layout_height="@dimen/_22sdp"
                        android:layout_marginEnd="10dp"
                        android:padding="@dimen/_1sdp"
                        android:layout_gravity="center_vertical"
                        android:gravity="center_vertical"
                        android:src="@drawable/vibrate_image"
                        app:tint="@color/wooden_dark_text" />

                    <LinearLayout
                        android:id="@+id/llVibrator"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:layout_weight="1"
                            android:orientation="vertical">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/Keypad_Touch_Vibration"
                                android:textColor="@color/wooden_dark_text"
                                android:textSize="@dimen/sip_15" />
                        </LinearLayout>
                        <com.hidefile.secure.folder.vault.edptrs.ComeBack
                            android:id="@+id/sw_vibreat"
                            android:layout_width="48dp"
                            android:layout_height="28dp"
                            android:layout_gravity="center"
                            app:sb_background="@color/colorLightGray2"
                            app:sb_checked_color="#0F3583F6"
                            app:sb_checkedbutton_color="@color/blue_ad"
                            app:sb_enable_effect="false"
                            app:sb_show_indicator="false"
                            app:sb_uncheckcircle_color="@color/wooden_dark_text" />
                    </LinearLayout>
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:layout_marginVertical="@dimen/_5sdp"
                    android:layout_marginLeft="@dimen/_30sdp"
                    android:background="@color/gray_solid" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_7sdp"
                    android:gravity="center"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="@dimen/_22sdp"
                        android:layout_height="@dimen/_22sdp"
                        android:layout_marginEnd="10dp"
                        android:padding="@dimen/_1sdp"
                        android:layout_gravity="center_vertical"
                        android:gravity="center_vertical"
                        android:src="@drawable/intruder_image"
                        app:tint="@color/wooden_dark_text" />
                    <LinearLayout
                        android:id="@+id/llHiddenSelfieSound"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_weight="1"
                            android:orientation="vertical">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/Intruder_Security"
                                android:textColor="@color/wooden_dark_text"
                                android:textSize="@dimen/sip_15" />
                        </LinearLayout>
                        <com.hidefile.secure.folder.vault.edptrs.ComeBack
                            android:id="@+id/swHiddenSelfieSound"
                            android:layout_width="48dp"
                            android:layout_height="28dp"
                            android:layout_gravity="center"
                            app:sb_background="@color/colorLightGray2"
                            app:sb_checked_color="#0F3583F6"
                            app:sb_checkedbutton_color="@color/blue_ad"
                            app:sb_enable_effect="false"
                            app:sb_show_indicator="false"
                            app:sb_uncheckcircle_color="@color/wooden_dark_text" />
                    </LinearLayout>
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginBottom="@dimen/_5sdp"
                    android:layout_marginLeft="@dimen/_30sdp"
                    android:background="@color/gray_solid" />



            </LinearLayout>
        </ScrollView>
</LinearLayout>

    <FrameLayout
        android:id="@+id/llNativeLarge"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_232sdp"
        android:layout_alignParentBottom="true"
        android:visibility="gone"
        android:layout_marginTop="@dimen/_8sdp"
        android:layout_marginBottom="@dimen/_3sdp"
        android:orientation="vertical">
        <include layout="@layout/ad_view_big" />
    </FrameLayout>
</RelativeLayout>