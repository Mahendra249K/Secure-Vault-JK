<?xml version="1.0" encoding="utf-8"?>
<com.hidefile.secure.folder.vault.cluecanva.FrSq xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"

    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/frm_root"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginEnd="@dimen/dim_2"
        android:layout_marginBottom="@dimen/dim_2"

        android:orientation="vertical">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.cardview.widget.CardView
                android:id="@+id/cardView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:cardPreventCornerOverlap="false"
                app:cardUseCompatPadding="false">

                <com.makeramen.roundedimageview.RoundedImageView
                    android:id="@+id/media_thumbnail"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="centerCrop"
                    android:src="@drawable/icf_import_file"
                    app:riv_corner_radius="0dp" />

                <ImageView
                    android:id="@+id/iv_chek"
                    android:layout_width="@dimen/dim_20"
                    android:layout_height="@dimen/dim_20"
                    android:layout_gravity="top|end"
                    android:layout_margin="@dimen/dim_5"
                    android:src="@drawable/check"
                    android:visibility="gone" />
            </androidx.cardview.widget.CardView>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical"
                android:visibility="gone">

                <com.hidefile.secure.folder.vault.cluecanva.VTv
                    android:id="@+id/tv_filename"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:text="adab"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_13sdp"/>

                <com.hidefile.secure.folder.vault.cluecanva.VTv
                    android:id="@+id/tv_size"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:text="adab"
                    android:textColor="@color/textColor2"
                    android:textSize="@dimen/_11sdp"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</com.hidefile.secure.folder.vault.cluecanva.FrSq>